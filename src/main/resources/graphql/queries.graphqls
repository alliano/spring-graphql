type Query {
    bookById(id: String) : Book
    getHelloWolrd(name: String) : String
    getStudentBiId(id: String) : Student
    getStudents : [Student]
}

type Book {
    id: String
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: String
    firstName: String
    lastName: String
}